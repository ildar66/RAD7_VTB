package com.vtb.domain;

import java.sql.Date;

/**
 * Organization is a one type of contractor.
 * This class contains information about organization. 
 * 
 * @author Tormozov M.G.
 *
 */
public class Organization extends VtbObject {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;


	public static final Long CONST_ID_UNKNOWN = null;
	
	
	Long id_contractor;
	String accountid;
	String account_name;
	String account_fullname;
	Long id_opf;
	String rating;
	Integer employees_count;
	String phone;
	String fax;
	String email;
	Date date_of_registration;
	String category;
	Long id_industry; 
	String business_description;
	Long id_region;
	String city;
	String law_address;
	String real_address;
	String postal_code;
	String INN;
	String KPP;
	String department;
	
	public Organization() {
		
	}
	
	public Organization(Long id, String crmID, String name) {
		this.id_contractor = id;
		this.accountid = crmID;
		this.account_name = name;
	}
	
	public String getAccount_fullname() {
		return account_fullname;
	}
	public void setAccount_fullname(String account_fullname) {
		this.account_fullname = account_fullname;
	}
	public String getAccount_name() {
		return account_name;
	}
	public void setAccount_name(String account_name) {
		this.account_name = account_name;
	}
	public String getAccountid() {
		return accountid;
	}
	public void setAccountid(String accountid) {
		this.accountid = accountid;
	}
	public String getBusiness_description() {
		return business_description;
	}
	public void setBusiness_description(String business_description) {
		this.business_description = business_description;
	}
	public String getCategory() {
		return category;
	}
	public void setCategory(String category) {
		this.category = category;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	public Date getDate_of_registration() {
		return date_of_registration;
	}
	public void setDate_of_registration(Date date_of_registration) {
		this.date_of_registration = date_of_registration;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public Integer getEmployees_count() {
		return employees_count;
	}
	public void setEmployees_count(Integer employees_count) {
		this.employees_count = employees_count;
	}
	public String getFax() {
		return fax;
	}
	public void setFax(String fax) {
		this.fax = fax;
	}
	public Long getId_contractor() {
		return id_contractor;
	}
	public void setId_contractor(Long id_contractor) {
		this.id_contractor = id_contractor;
	}
	public Long getId_opf() {
		return id_opf;
	}
	public void setId_opf(Long id_opf) {
		this.id_opf = id_opf;
	}
	public Long getId_industry() {
		return id_industry;
	}
	public void setId_industry(Long industryID) {
		this.id_industry = industryID;
	}
	public String getINN() {
		return INN;
	}
	public void setINN(String inn) {
		INN = inn;
	}
	public String getKPP() {
		return KPP;
	}
	public void setKPP(String kpp) {
		KPP = kpp;
	}
	public String getLaw_address() {
		return law_address;
	}
	public void setLaw_address(String law_address) {
		this.law_address = law_address;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public String getPostal_code() {
		return postal_code;
	}
	public void setPostal_code(String postal_code) {
		this.postal_code = postal_code;
	}
	public String getRating() {
		return rating;
	}
	public void setRating(String rating) {
		this.rating = rating;
	}
	public String getReal_address() {
		return real_address;
	}
	public void setReal_address(String real_address) {
		this.real_address = real_address;
	}
	public Long getId_region() {
		return id_region;
	}
	public void setId_region(Long regionID) {
		this.id_region = regionID;
	}

	public Organization(Integer id_contractor) {
		super();
		this.id_contractor = new Long(id_contractor);
	}

	public String getDepartment() {
		return department;
	}

	public void setDepartment(String departament) {
		this.department = departament;
	}

	
	
}
